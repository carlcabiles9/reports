
<h1>Reports</h1>
<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |reports| %>
      <tr>
         @reports.projects.each do |s|
            <td> s.name</td>
         end 
        <td> @reports.statement</td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
